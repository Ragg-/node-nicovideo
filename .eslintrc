{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "impliedStrict": true
        },
    },
    "rules": {
        "no-console": 2,
        "no-unused-expressions": 2,
        "no-case-declarations": 0
    }
}
